/** community boards */
#nycd{
  polygon-fill: #2dc103;
  polygon-opacity: 0;
  line-color: #a53ed5;
  line-width: 0.4;
  line-opacity: 1;
}

#nycd::labels {
  text-name: [borocd];
  text-face-name: 'Raleway';
  text-size: 14;
  text-label-position-tolerance: 0;
  text-fill: #a53ed5;
  text-halo-fill: #333333;
  text-halo-radius: 1;
  text-dy: 0;
  text-allow-overlap: true;
  text-placement: interior;
  text-placement-type: simple;
}
