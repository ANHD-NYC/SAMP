/* city council districts */ 
#nycc{
  polygon-fill: #FF6600;
  polygon-opacity: 0;
  line-color: #5CA2D1;
  line-width: 0.7;
  line-opacity: 0.8;
}

#nycc::labels {
  text-name: [coundist];
  text-face-name: 'DejaVu Sans Bold';
  text-size: 14;
  text-label-position-tolerance: 0;
  text-fill: #5CA2D1;
  text-halo-fill: #333333;
  text-halo-radius: 1;
  text-allow-overlap: true;
  text-placement: interior;
  text-placement-type: simple;
}
