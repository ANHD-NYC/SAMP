/** zip codes */
#zipcode{
  polygon-fill: #2dc103;
  polygon-opacity: 0;
  line-color: #229A00;
  line-width: 2;
  line-opacity: 1;
}
#zipcode::labels {
  text-name: [zipcode];
  text-face-name: 'DejaVu Sans Bold';
  text-size: 14;
  text-label-position-tolerance: 0;
  text-fill: #229A00;
  text-halo-fill: #333333;
  text-halo-radius: 1;
  text-dy: 0;
  text-allow-overlap: true;
  text-placement: interior;
  text-placement-type: simple;
}
